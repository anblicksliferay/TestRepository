<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="com.astra.anblicks.pdca">

	<namespace>pdca</namespace>
	
	<!-- LookUp Tables for Static Data -->
	
	<entity local-service="true" name="type" remote-service="true" data-source="pdcaSource" uuid="true" table="pdca_type">
	   <column name="typeId" primary="true" type="long" />
	   <column name="typeName" type="String" /> 
	</entity>
	
	
	<entity local-service="true" name="itemDescription" remote-service="true" data-source="pdcaSource" uuid="true"  table="pdca_itemdescription">
	   <column name="itemDescriptionId" primary="true" type="long" />
	   <column name="itemDescriptionName" type="String" />
	</entity>
	
	
	<entity local-service="true" name="achivementFormula" remote-service="true" data-source="pdcaSource" uuid="true" table="pdca_achivementformula">
	   <column name="achivementFormulaId" primary="true" type="long" />
	   <column name="achivementFormula" type="String" />
	   <column name="itemDescriptionId" type="long" />
	</entity>
	
	
	<entity local-service="true" name="period" remote-service="true" data-source="pdcaSource" uuid="true" table="pdca_period">
	   <column name="periodId" primary="true" type="long" />
	   <column name="periodName" type="String" />
	</entity>
	
	
	<entity local-service="true" name="tradingProfit" remote-service="true" data-source="pdcaSource" uuid="true" table="pdca_tradingprofit">
	   <column name="tradingProfitId" primary="true" type="long" />
	   <column name="periodId" type="long" />
	   <column name="companyId" type="long" />
	   <column name="year" type="long" />
	   <column name="npat" type="double" />
	   <column name="netForex" type="double" />
	   <column name="ppeDispos" type="double" />
	   <column name="revalutionOnPropertyInvestment" type="double" />
	   <column name="investment" type="double" />
	   <column name="taxExpense" type="double" />
	   <column name="impairmentOnAsset" type="double" />
	   <column name="others" type="double" />
	   <column name="TotalSum" type="double" />
	   <column name="notes_Npat" type="String" />
	   <column name="notes_NetForex" type="String" />
	   <column name="notes_ppeDispos" type="String" />
	   <column name="notes_RevalutiononPI" type="String" />
	   <column name="notes_Investment" type="String" />
	   <column name="notes_taxExpenses" type="String" />
	   <column name="notes_impartmentOnAsset" type="String" />
	   <column name="notes_others" type="String" />
	</entity>
	
	<!-- End of LookUp Tables -->
	
	<!-- Master Tables For CLA Module -->
	
	<entity local-service="true" name="kpi" remote-service="true" data-source="pdcaSource" uuid="true" table="pdca_kpi">
	   <column name="kpiId" primary="true" type="long" />
	   <column name="companyId" type="long" />
	   <column name="typeId" type="long" />
	   <column name="weight" type="long" />
	   <column name="target" type="long" />
	   <column name="year" type="long" />
	   <column name="unit" type="String" />
	   <column name="achivementFormulaId" type="long" />
	   <column name="itemDescriptionName" type="String" />
	   	   <column name="itemDescriptionValue" type="String" />
	   <!-- Audit fields for User-->
	   
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
	</entity>
	
	<entity local-service="true" name="cla_kpi" remote-service="true" data-source="pdcaSource" uuid="true" table="pdca_cla_kpi">
	    <column name="claKpiId" primary="true" type="long" />
	    <column name="kpiId" type="long" />
	    <column name="periodId" type="long" />
	    <column name="ytd" type="long" />
	    <column name="ol" type="double" />
	    <column name="claPoint" type="double" />
	    <column name="achivement" type="double" />
		 <column name="peroidInput" type="String"></column>
	</entity>
	
	<!-- End Of Master Tables Of CLA Module -->
