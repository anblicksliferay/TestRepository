	Assume Sample Company Table
	<entity local-service="true" name="company" remote-service="true" data-source="pdcaSource" uuid="true" table="pdca_company">
	    <column name="companyId" primary="true" type="long" />
	    <column name="companyName" type="String" />
	</entity> -->
	
	<!-- End of Company Table -->
	
		<!-- End of Company Table -->
	
	<entity local-service="true" name="AstraPdca_CompanyData" remote-service="true"  data-source="pdcaSource" table="pdca_astrapdca_companydata" >
	<column name="companyId" type="long" primary="true"></column>
	<column name="CompanyCode" type="String"></column>
	<column name="CompanyName" type="String"></column>
	<column name="GroupName" type="String"></column>
	<column name="DIC" type="String"></column>
	<column name="DICINITIAL" type="String"></column>
	<column name="LEFTUP" type="String"></column>
	<column name="LEFTUPTITLE" type="String"></column>
	<column name="LEFTUPCOMPANY" type="String"></column>
	<column name="LEFTBOTTOM" type="String"></column>
	<column name="LEFTBOTTOMTITLE" type="String"></column>
	<column name="LEFTBOTTOMCOMPANY" type="String"></column>
	<column name="MIDUP" type="String"></column>
	<column name="MIDUPTITLE" type="String"></column>
	<column name="MIDUPCOMPANY" type="String"></column>
	<column name="MIDBOTTOM" type="String"></column>
	<column name="MIDBOTTOMTITLE" type="String"></column>
	<column name="MIDBOTTOMCOMPANY" type="String"></column>
	<column name="RIGHTUP" type="String"></column>
	<column name="RIGHTUPTITLE" type="String"></column>
	<column name="RIGHTUPCOMPANY" type="String"></column>
	<column name="RIGHTBOTTOM" type="String"></column>
	<column name="RIGHTBOTTOMTITLE" type="String"></column>
	<column name="RIGHTBOTTOMCOMPANY" type="String"></column>
	<column name="ModuleId" type="long"></column>  <!-- //this column should map pdca_moudule teable -->
	<column name="YearId" type="long"></column>  <!-- //this column should map pdca_year teable -->
	<column name="CheckFlag" type="String"></column>
	
	<!-- Aduit columns -->
	<column name="SiteId" type="long"></column>
	<column name="CreateDate" type="Date"></column>
	<column name="CreatedBy" type="String"></column>
	<column name="ModifiedDate" type="Date"></column>
	<column name="ModifiedBy" type="String"></column>
	</entity>
	
		<entity local-service="true" name="AstraPdca_Year" remote-service="true" data-source="pdcaSource"  table="pdca_astrapdca_year">
		 
		 <column name="YearId" type="long" primary="true" ></column>
		 <column name="YearId_Company" type="long"></column>   <!-- mapped column to "AstraPdca_CompanyData" table -->
          <column name="Year" type="long"></column>
	<!-- Aduit columns -->
	<column name="SiteId" type="long"></column>
	<column name="CreateDate" type="Date"></column>
	<column name="CreatedBy" type="String"></column>
	<column name="ModifiedDate" type="Date"></column>
	<column name="ModifiedBy" type="String"></column>
		</entity>
	<entity local-service="true" name="AstraPdca_Module" remote-service="true" data-source="pdcaSource" table="pdca_astrapdca_module" >
		 
		 <column name="ModuleId" type="long" primary="true" ></column>
		 <column name="ModuleId_Company" type="long"></column>   <!-- mapped column to "AstraPdca_CompanyData" table -->
          <column name="ModuleName" type="String"></column>
          <column name="checkUncheckFlag" type="String"></column>
	<!-- Aduit columns -->
	<column name="SiteId" type="long"></column>
	<column name="CreateDate" type="Date"></column>
	<column name="CreatedBy" type="String"></column>
	<column name="ModifiedDate" type="Date"></column>
	<column name="ModifiedBy" type="String"></column>
		</entity>
	
	
	
	<!-- Mamny to Many Relationship  -->
	<entity name="company_user" local-service="true" remote-service="true"  data-source="pdcaSource" table="pdca_company_user">
	<column name="company_user_Id" type="long" primary="true"></column>
	<column name="companyId" type="long" ></column>
	<column name="Pdca_userId" type="long"></column>
	
	<finder return-type="collection" name="getByUserList">
	<finder-column name="companyId"></finder-column>
	</finder>
	
	<finder return-type="collection" name="getByCompanyList">
	<finder-column name="companyId"></finder-column>
	</finder>
	
	</entity>
	
	<entity name="pdca_AdminSettings" local-service="true" remote-service="true"  data-source="pdcaSource" table="pdca_pdca_adminsettings">
	
	
	<column name="settings_Id" type="long" primary="true"></column>
	<column name="moduleId" type="long"></column>
	<column name="year" type="long"></column>
	<column name="companyId" type="long"></column>
<column name="periodId" type="long"></column>
<!-- Aduit columns -->
	<column name="SiteId" type="long"></column>
	<column name="CreateDate" type="Date"></column>
	<column name="CreatedBy" type="String"></column>
	<column name="ModifiedDate" type="Date"></column>
	<column name="ModifiedBy" type="String"></column>
	</entity>
	
	<!-- MOM Tables -->
	<entity name="ListMOM"  local-service="true" remote-service="true"  data-source="pdcaSource" table="pdca_listmom">
	<column name="MomId" type="long"  primary="true"></column>
	<column name="userId" type="long"></column>
	<column name="companyId" type="long"></column>
	<column name="meetingDate" type="String"></column>
	<column name="fromTime" type="String" ></column>
	<column name="ampm" type="String"></column>
	<column name="MomType" type="String"></column>
	<column name="MomTypeId" type="String"></column>
	<column name="performance" type="String"></column>
	<column name="MOMGroup" type="String"></column>
	<column name="MOMparticipants" type="String"></column>
	<column name="MOMcompanyName" type="String"></column>
	<column name="MOMplace" type="String"></column>
	<column name="ponitDiscussionId" type="long"></column>  <!-- //this column should mapped with MOMPointdiscussoin table -->
	<!-- Aduit columns -->
	<column name="SiteId" type="long"></column>
	<column name="CreateDate" type="Date"></column>
	<column name="CreatedBy" type="String"></column>
	<column name="ModifiedDate" type="Date"></column>
	<column name="ModifiedBy" type="String"></column>	
	</entity>
	
	<entity name="MOMPointdiscussoin" local-service="true" remote-service="true"  data-source="pdcaSource" table="pdca_mompointdiscussoin">
	<column name="PdId" type="long" primary="true"></column>
	<column name="MOMpdId" type="long"></column>  <!-- //this column should mapped with ListMOM table -->
	<column name="PonintDiscussionName" type="String"></column>
	<column name="Notes" type="String"></column>
	
	<!-- Aduit columns -->
	<column name="SiteId" type="long"></column>
	<column name="CreateDate" type="Date"></column>
	<column name="CreatedBy" type="String"></column>
	<column name="ModifiedDate" type="Date"></column>
	<column name="ModifiedBy" type="String"></column>	
	</entity>
	
	<entity name="ControlMOM"  local-service="true" remote-service="true"  data-source="pdcaSource" table="pdca_controlmom">
	
	<column name="ControlMOMId" type="long" primary="true"></column>
	<column name="ListMOMId" type="long"></column> 
	<column name="pdId" type="long"></column> 
	<column name="userId" type="long"></column> <!-- Shared User Id -->
	<column name="Status" type="String"></column>
	<column name="FollowUp" type="String"></column>
	<column name="FollowUpData" type="String"></column>
	
	<!-- Aduit columns -->
	<column name="SiteId" type="long"></column>
	<column name="CreateDate" type="Date"></column>
	<column name="CreatedBy" type="String"></column>
	<column name="ModifiedDate" type="Date"></column>
	<column name="ModifiedBy" type="String"></column>	
	</entity>
	
	
</service-builder>